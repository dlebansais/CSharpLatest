{
    "stryker-config": {
        "ignore-methods": [
            "ConfigureAwait", // Ignores all ConfigureAwait
            "EnableConcurrentExecution", // Ignores enabling concurrent execution
            "Assert*", // Ignores all assertions
            "Unused", // Ignores all unused parameters
            "Require*", // Ignores all preconditions
            "Ensure*" // Ignores all postconditions
        ],
        "test-projects": [
            "CSharpLatest.PropertyGenerator.Test/CSharpLatest.PropertyGenerator.Test.csproj",
            "CSharpLatest.Test/CSharpLatest.Test.csproj"
        ],
        "target-framework": "net9.0"
    }
}